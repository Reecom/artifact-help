package com.yunat.maven.plugin.plugins;import org.apache.commons.exec.CommandLine;public class CommandLineUtils {	public static CommandLine artifact(String fileDir, String line) {		CommandLine commandLine = null;		String os = System.getProperty("os.name");		if (os != null) {			if (os.startsWith(OSSerialType.Mac.toString())) {				commandLine = new CommandLine("bash");				commandLine.addArgument("-c");				commandLine.addArgument("cd " + fileDir);				commandLine.addArgument("bash");				commandLine.addArgument("-c");				commandLine.addArgument(line);							} else if (os.startsWith(OSSerialType.Windows.toString())){				commandLine = new CommandLine("cmd");				commandLine.addArgument("/c");				commandLine.addArgument("cd " + fileDir);				commandLine.addArgument("cmd");				commandLine.addArgument("/c");				commandLine.addArgument(line);			}		}		return commandLine;	}}