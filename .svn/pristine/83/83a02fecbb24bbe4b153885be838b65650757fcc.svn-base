package com.yunat.maven.plugin.plugins;import org.apache.commons.exec.CommandLine;public class CommandLineUtils {	public static CommandLine artifact(String fileDir, String line) {		CommandLine commandLine = null;		String os = System.getProperty("os.name");		if (os != null) {			if (os.startsWith(OSSerialType.Mac.toString())) {				commandLine = CommandLine.parse("bash -c " + line);			} else if (os.startsWith(OSSerialType.Windows.toString())){				commandLine = CommandLine.parse("cmd /c " + line);			}		}		return commandLine;	}}